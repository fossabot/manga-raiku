  "description": "Free and ad-free private online Manga, manhua, manhwa reading app!",
  "productName": "Manga Raiku",
  "author": "Tachibana Shin <45375496+tachibana-shin@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "pretit": "prettier \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "pnpm test:unit:ci && pnpm test:component:ci",
    "test:watch": "pnpm test:unit",
    "dev": "quasar dev",
    "typing": "vue-tsc --noEmit",
    "build": "quasar build",
    "ci": "pnpm format && pnpm lint --fix && pnpm typing && pnpm test:unit:ci",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "cypress:open": "cypress open",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
